{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PYTHONHOME",
                    "value": "E:/miniconda/envs/cv"
                },
                {
                    "name": "PATH",
                    "value": "E:/miniconda/envs/cv;${env:PATH}"
                }
            ],
            "externalConsole": false,
            "preLaunchTask": "build with python"
        }
    ]
}